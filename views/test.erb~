		<script src="//ajax.googleapis.com/ajax/libs/dojo/1.9.0/dojo/dojo.js" data-dojo-config="async:true, parseOnLoad: true"></script>
		<script>
			require(["dojo/request/script", "dojo/dom-construct", "dojo/domReady!"],
				function(script, domConstruct){
					script.get("test.json", {
						jsonp: "callback",
						query: { q: "#dojo" }
					});
			});
